{
  "name": "nextjs-starter-template",
  "version": "0.1.0",
  "private": true,
  "repository": "https://github.com/vashafaris/nextjs-starter-template",
  "author": "Vasha Farisi <vashafarisi@gmail.com>",
  "license": "Unlicense",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "format": "prettier --write \"./**/*.{ts,tsx}\"",
    "lint": "eslint \"**/*.{ts,tsx}\" \"*.config.js\"",
    "prepare": "husky install",
    "validate": "npm run type-check && npm run lint",
    "test": "jest --watch",
    "test:ci": "jest --ci"
  },
  "dependencies": {
    "clsx": "^1.1.1",
    "isomorphic-unfetch": "^3.1.0",
    "lodash": "^4.17.21",
    "next": "12.1.0",
    "nprogress": "^0.2.0",
    "query-string": "^7.1.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "swr": "^1.2.2",
    "zustand": "^3.7.1"
  },
  "devDependencies": {
    "@babel/eslint-plugin": "^7.16.5",
    "@commitlint/cli": "^16.2.1",
    "@commitlint/config-conventional": "^16.2.1",
    "@testing-library/jest-dom": "5.16.1",
    "@testing-library/react": "12.1.2",
    "@testing-library/user-event": "13.5.0",
    "@types/node": "17.0.21",
    "@types/nprogress": "^0.2.0",
    "@types/react": "17.0.40",
    "@typescript-eslint/eslint-plugin": "^5.14.0",
    "@typescript-eslint/parser": "^5.14.0",
    "autoprefixer": "^10.4.2",
    "babel-jest": "27.4.5",
    "eslint": "^8.11.0",
    "eslint-config-kentcdodds": "^20.1.0",
    "eslint-config-next": "^12.1.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.3",
    "husky": "^7.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "27.4.5",
    "lint-staged": "^12.3.5",
    "postcss": "^8.4.8",
    "prettier": "^2.5.1",
    "stylelint": "^14.5.3",
    "stylelint-config-recommended": "^7.0.0",
    "tailwindcss": "^3.0.23",
    "typescript": "4.6.2"
  },
  "engines": {
    "node": ">=14.0.0",
    "yarn": ">=1.22.0",
    "npm": "please-use-yarn"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx,html,css,json}": [
      "yarn prettier --write"
    ]
  }
}
